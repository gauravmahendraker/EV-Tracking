                  <% } %>
              <% } else if(locals.driverId !=null) { %>
                <% if(driverId != null) { %>
                  <li class="nav-item">
                    <a type="button" class="btn" href="/">Logout as Driver</a>
    
                  </li>
                  <% } else { %>
                    <li class="nav-item">
                      <button type="button" class="btn navbut" data-bs-toggle="modal" data-bs-target="#user-modal">Login as Driver</button>
